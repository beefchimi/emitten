{
  "env": {
    "browser": true,
    "es2022": true
  },
  "extends": ["standard-with-typescript", "prettier"],
  "plugins": ["prettier"],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module",
    "project": "./tsconfig.json"
  },
  "rules": {
    "prettier/prettier": ["error"],
    "@typescript-eslint/explicit-function-return-type": "off",
    "@typescript-eslint/consistent-indexed-object-style": [
      "error",
      "index-signature"
    ]
  }
}
